#!/usr/bin/env bash
set -eux;

# Compute script directory.
readonly package_dir="$( cd $(dirname ${BASH_SOURCE})/..; pwd )";

cd "${package_dir}";
ssh mf killall collector || true;
cargo build --release --bin collector;
scp target/release/collector mf:/home/alex/lib/monitor/collector;
ssh mf 'RUST_LOG="collector=trace" /home/alex/lib/monitor/collector';
